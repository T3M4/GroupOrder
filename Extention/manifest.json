{
    "name": "Group Order",
    "version": "1.0",
    "description": "This extension can help you make group order",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "content_scripts": [
        {
            "matches": ["*://misteram.com.ua/*"],
            "js": ["content.js"]
        }
    ],
    "page_action": {
        "default_popup": "./homePage/home.html",
        "default_title": "GroupOrder"
    },
    "permissions": [
        "activeTab",
        "<all_urls>",
        "tabs",
        "webRequestBlocking",
        "webRequest"
    ]
}